{
  "name": "react-video-component",
  "version": "1.0.2",
  "description": "Simple video component for react",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "build-dist": "webpack --config webpack.dist.config.js",
    "build-demo": "webpack --config webpack.demo.config.js",
    "dev": "concurrently \"webpack -wd\" \"webpack --config webpack.demo.config.js -wd\"",
    "deploy": "concurrently \"npm run build\" \"npm run build-dist\" \"npm run build-demo\""
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-html-attrs",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wufe/React-video-component.git"
  },
  "keywords": [
    "video",
    "react",
    "component",
    "html5"
  ],
  "author": "Wufe <simone.bembi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wufe/React-video-component/issues"
  },
  "homepage": "https://github.com/Wufe/React-video-component#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "moment": "^2.13.0",
    "radium": "^0.17.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.1"
  }
}
